///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
// 6. COMPONENTS //////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
$form_border_color:       $border_color;
$form_border_color_focus: $color_2;
$form_border_radius:      3px;

$label_font_size:         $base_font_size;
$label_font_color:        $base_font_color;

$input_font_size:         $base_font_size;
$input_font_color:        $base_font_color;

$alert_info_color:        #c3e5ff;
$alert_warning_color:     #ffedc3;
$alert_error_color:       #ffc3c3;




// 6.1. FORMS /////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
label {
	display: block;
	font-size: $label_font_size;
	color: $label_font_color;
	cursor: pointer;
}

[type="text"].form_control,
[type="email"].form_control,
[type="search"].form_control,
[type="password"].form_control,
select.form_control,
textarea.form_control {
	width: 100%;
	height: 44px;
	padding: 0 12px;
	border: 1px solid $form_border_color;
	outline: none;
	font: $input_font_size $base_font;
	color: $input_font_color;
	text-indent: 0;

	-webkit-appearance: none;
	   -moz-appearance: none;
			appearance: none;

	-webkit-border-radius: $form_border_radius;
			border-radius: $form_border_radius;

	&:focus {
		border-color: $form_border_color_focus;
	}

}

select.form_control {
	background: url( $icon_angle_down ) calc( 100% - 12px ) 50% no-repeat;
	background-size: 10px 10px;
}
@-moz-document url-prefix() {
	select.form_control {
		text-indent: -2px;
	}
}

textarea[type="text"].form_control {
	min-width: 100%;
	max-width: 100%;
	width: 100%;
	min-height: 100px;
	height: 100px;
	padding: 12px;
}

[type="checkbox"].form_control,
[type="radio"].form_control {
	display: none;

