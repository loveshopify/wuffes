// 6.2. ALERTS ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
.alert {
	padding: 15px;
	border: 1px solid transparent;
	border-radius: $form_border_radius;
}

.alert_info {background: $alert_info_color;border-color: darken( $alert_info_color, 20% );color: darken( $alert_info_color, 55% );}

.alert_warning {background: $alert_warning_color;border-color: darken( $alert_warning_color, 20% );color: darken( $alert_warning_color, 55% );}

.alert_error {background: $alert_error_color;border-color: darken( $alert_error_color, 20% );color: darken( $alert_error_color, 55% );}




// 6.3. BUTTONS ///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////




// 6.4. TABLES ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
table {
	border-collapse: collapse;
}

th {
	text-align: inherit;
}




